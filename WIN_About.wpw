     type_code : 14
     code : |1+
      // Summary: Adapts the window size to the content of the text control
      // Syntax:
      //WinMobileAdaptSize (<sControlName> is string)
      //
      // Parameters:
      //	sControlName (UNICODE string): Name of the control containing the long text
      // Return value:
      // 	None
      //
      PROCEDURE MobileWinAdaptSize(sControlName is string)
      
      nBottomMargin is int = CoordinateEditorToScreen(10)
      nSecurityMargin is int = CoordinateEditorToScreen(20)
      
      IF IniOSMode() THEN
      	nSecurityMargin = CoordinateEditorToScreen(40)
      END
      
      // Check the control existence
      IF ControlExist(sControlName)=False THEN
      	RETURN
      END
      
      // Resize the control
      nTextHeight is int= TextHeight(sControlName,{sControlName,indControl}..Caption,tMultiline) + nSecurityMargin
      IF {sControlName,indControl}..X + nTextHeight + nBottomMargin < MyWindow..InitialHeight THEN
      	nTextHeight = MyWindow..InitialHeight - nBottomMargin - {sControlName,indControl}..X
      END
      
      {sControlName,indControl}..Height = nTextHeight
      
      // Resize the window
      nWindowHeight is int
      IF {sControlName,indControl}..X + nTextHeight + nBottomMargin > MyWindow..InitialHeight THEN
      	//nWindowHeight = {sControlName,indControl}..X + nTextHeight + nBottomMargin
      	nWindowHeight = MyWindow..InitialHeight + (nTextHeight - {sControlName,indControl}..InitialHeight)
      ELSE
      	nWindowHeight = MyWindow..InitialHeight
      END
      
      
      MyWindow..UsefulHeight = nWindowHeight
      
     type : 458752
