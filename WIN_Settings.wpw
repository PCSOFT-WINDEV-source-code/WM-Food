  procedures :
   -
     name : ValidateInput
     procedure_id : 1583380152286150574
     type_code : 14
     code : |1+
      // Summary: Checks the user inputs
      // Syntax:
      // ValidateInput ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE ValidateInput()
      
      // Deletes the spaces added by the automatic input (in iOS for example)
      EDT_Input = NoSpace(EDT_Input)
      
      IF EDT_Input = "" THEN
      	Error("Enter an allergen.")
      	RETURN
      END
      
      FOR EACH sAllergen of SET_OpenFoodFacts.garrAllergens
      	IF StringCompare(sAllergen,EDT_Input,ccIgnoreAccent+ccIgnoreCase+ccIgnoreSpace+ccIgnorePunctuationAndSpace) = 0 THEN
      		// Allergen already added
      		Error("This ingredient was already added.")	
      		RETURN
      	END
      END
      
      Add(SET_OpenFoodFacts.garrAllergens,EDT_Input)
      fSaveText(FILE_INGREDIENTS,ArrayToString(SET_OpenFoodFacts.garrAllergens,CR))
      EDT_Input = ""
      
      RefreshLPR()
      ToastDisplay("Allergen added")
      
     type : 458752
   -
     name : RefreshLPR
     internal_properties : CAAAAAgAAABCZJIod4EViR9OvnRjeuFtGOZfZIJdcVXNsrDLowPC5jwvKeDSiVFTq4ckCN2CUAI55OzdU1i1ex4kn1LtUGgin1E1GWSBkC0povbmFMN/pQc5EDA7CopaQeIzhP/mh+06TASqQnPyahsiMsaLrVWRa4TBSrFO0KbwifbT1Fqf74oJAlQ2ua/JHF3FVLF3Zsl3O/rTYw==
     procedure_id : 1583380152286216110
     type_code : 14
     code : |1-
      // Summary: Refresh the display of the looper
      // Syntax:
      // RefreshLPR ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      PROCEDURE RefreshLPR()
      
      LooperDeleteAll(LOOP_Allergies)
      FOR EACH sAllergen of SET_OpenFoodFacts.garrAllergens
      	LooperAddLine(LOOP_Allergies,sAllergen)
      END
      
      // Hides the caption if at least one allergen is found
      STC_None..Visible = (SET_OpenFoodFacts.garrAllergens..Occurrence=0)
     type : 458752
